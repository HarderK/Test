<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title><%= title %></title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/photo.css">
</head>
<body>
	<h1>Photos</h1>
	<p>Express stock photo application.</p>
	<div class="photos">
		<% photos.forEach(function(item){ %>
			<div class="photo">
				<h2><%= item.name %></h2>
				<a href="/photos/photo/<%= item.id %>/download">
					<img src="<%= item.path %>">
				</a>
			</div>
		<% }) %>
	</div>
</body>
</html>